nk :to=\"{ name: 'Login' }\">Login</router-link>\n      </a>\n      <a>\n        <router-link :to=\"{ name: 'Register' }\">Registro</router-link>\n      </a>\n\n      <a>\n        <router-link :to=\"{ name: 'Allconcourses' }\">Todos los concursos</router-link>\n      </a>\n      <a>\n        <router-link :to=\"{ name: 'About' }\">Contacto</router-link>\n      </a>\n      <a>\n        <router-link :to=\"{ name: 'Profile' }\">Mi Perfil</router-link>\n      </a>\n      <!-- No funciona PENDIENTE DE REVISAR -->\n      <div class=\"verboton\" v-show=\"!seeButton\">\n        <!--Botón que llama a la función de LogOut -->\n\n        <button @click=\"logoutUser()\">Logout</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {clearLogin} from \"../api/utils\";\n\n\nexport default {\n    name: \"menucustom\",\n\n    return:{\n      seeButton: false,\n    },\n\n\nmethods:{\n  //Función de LogOut\n  logoutUser(){\n  \n    //Nos lleva al login\n    this.$router.push(\"/\");\n     // No deja datos en el logueo, vacío.\n    self.seeButton = true;\n    return clearLogin();\n    \n   \n  },\n \n},\n\n}\n</script>\n\n<style scoped>\n#contenedorMenu {\n  width: 80%;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n  margin: 0 auto;\n}\n</style>"]}]}