AAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAAA,WAAA,EAAA,CAFA,IAGA,QAAA,CAAA,MAAA,CAAA,WAAA,GAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAAA,WAAA,EAAA,CAJA;AAAA,OADA,CAMA;AACA;AAPA,OAAA;AASA;AAnBA,GAnBA;AA2CA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,kBAFA,8BAEA,QAFA,EAEA;AAEA,UAAA,IAAA,GAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EAHA,CAIA;;AACA,UAAA,KAAA,GAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,GAAA,YAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,MAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA,CAAA,eAAA,qBAAA,KAAA;AAGA,MAAA,KAAA,CAAA,IAAA,CAAA,mDAAA,KAAA,QAAA,CAAA,WAAA,EAAA;AACA,QAAA,WAAA,EAAA,IAAA,CAAA,WADA;AAEA,QAAA,UAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AAEA,QAAA,OAAA,CAAA,GAAA,CAAA,UAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EAHA,CAIA;;AACA,QAAA,IAAA,CAAA,IAAA,CAAA;AACA,UAAA,KAAA,EAAA,GADA;AAEA,UAAA,IAAA,EAAA,0CAFA;AAGA,UAAA,iBAAA,EAAA,KAHA;AAIA,UAAA,KAAA,EAAA;AAJA,SAAA;AAOA,OAhBA,EAiBA;AAjBA,OAkBA,KAlBA,CAkBA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AAEA,QAAA,IAAA,CAAA,IAAA,CAAA;AACA,UAAA,KAAA,EAAA,IADA;AAEA,UAAA,IAAA,EAAA,sDAFA;AAGA,UAAA,iBAAA,EAAA,KAHA;AAIA,UAAA,KAAA,EAAA;AAJA,SAAA;AAMA,OA3BA;AAgCA;AA5CA;AA3CA,CAAA;AA0FA","sourcesContent":["\n<template>\n  <div>\n    <!--Encabezado de página -->\n    <h2>Todos los concursos hasta la fecha</h2>\n    <!-- Forumulario de búsqueda -->\n    <div id=\"formulariobusqueda\">\n      <label for=\"bySearch\">Búsqueda</label>\n      <br />\n      <input\n        v-model=\"search\"\n        id=\"search\"\n        name=\"bySearch\"\n        type=\"search\"\n        placeholder=\"🔍 Nombre/Descripción...\"\n      />\n    </div>\n\n    <div class=\"contenedor\">\n      <br />\n      <!-- Contenedor para dar formato a la ficha de productos, lo cargamos con el array de productos filtrados -->\n      <div class=\"concursoscontenedor\" v-for=\"concurso in concursosFiltrados\" :key=\"concurso.id\">\n        <p>\n          <b>id.:</b>\n          {{concurso.id_concurso}}\n        </p>\n        <p>\n          <img :src=\"concurso.url_foto\" />\n        </p>\n        <p>\n          <b>Nombre:</b>\n          {{concurso.nombre}}\n        </p>\n        <p>\n          <b>Abierto a suscripción desde:</b>\n          {{concurso.fecha_inicio}}\n        </p>\n\n        <!-- REVISAR estas clases dinámicas dependientes de FECHA -->\n        <p\n          :class=\"{ green: concurso.fecha_final >= 'datetoday' , red: concurso.fecha_final <= 'datetoday' }\"\n        >\n          <b>Cierre de suscripción:</b>\n          {{concurso.fecha_final}}\n        </p>\n\n        <p>\n          <b>Información y bases:</b>\n          {{concurso.descripcion}}\n        </p>\n        <p>\n          <b>Modalidad:</b>\n          {{concurso.modalidad}}\n        </p>\n        <p>\n          <b>Genero:</b>\n          {{concurso.genero}}\n        </p>\n        <p>\n          <b>Ciudad:</b>\n          {{concurso.ciudad}}\n        </p>\n        <p>\n          <b>Valoración media:</b>\n          {{concurso.valoracion}}\n        </p>\n\n        <!-- Con una clase dinámica manejo los colores en función de la vigencia del concurso -->\n        <hr />\n        <!-- El botón de inscribir DE MENTIRIJILLAS AUN, hace una llamada a la función que nos envía un Sweet Alert -->\n        <button @click=\"confirmInscription(concurso)\">INSCRIBIRME</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// IMPORTAMOS PARA\n// enviar mensajes custom\nimport Swal from \"sweetalert2\";\n// manejo de rutas y endpoints\nimport axios from \"axios\";\n// componentes internos\n\n\nexport default {\nname: 'listallconcourses',\nprops:{\n  // Le indicamos que está recibiendo un array.\nconcursos: Array,\n\n\n},\n\ndata(){\n  return {\n    // Inicializamos un string vacío que contendrá la búsqueda.\n    search:\"\",\n    datetoday: new Date(),\n    id: null,\n    \n}\n},\n\ncomputed: {\n  \n  //FUNCIÓN PARA FILTRAR PRODUCTOS\nconcursosFiltrados() {\n     \n      // Si en la búsqueda no hay nada nos devuelve todo.\n  if (!this.search) {\n        return this.concursos;\n        console.log(this.concursos);\n      }\n return this.concursos.filter ( \n        concurso  => \n        concurso.nombre.toLowerCase().includes(this.search.toLowerCase()) ||\n        concurso.descripcion.toLowerCase().includes(this.search.toLowerCase()) ||\n        concurso.modalidad.toLowerCase().includes(this.search.toLowerCase()) ||\n        concurso.genero.toLowerCase().includes(this.search.toLowerCase()) \n        //Ojo aquí, pendiente que funcione la búsqueda por ciudad.\n        // concurso.ciudad.toLowerCase().includes(this.search.toLowerCase()) \n )\n    },\n  \n\n},\n\nmethods:{\n// Método para el botón de \"comprar\"\nconfirmInscription(concurso){\n\nconst self = this;\nconsole.log(concurso);\n            // Cojo token e id.\nconst token = localStorage.getItem(\"token\");\nconst data = localStorage.getItem(\"id\");\naxios.defaults.headers.common[\"Authorization\"] = `Bearer ${token}`;\n\n               \naxios.post (\"http://localhost:3003/concursos/inscripciones/\" + this.concurso.id_concurso,{\n            id_concurso: self.id_concurso,\n            id_usuario: data,\n})\n            .then(function(response) {\n\n                    console.log(\"login ok\")\n                    console.log(response);\n                     // Enviamos mensaje de confirmación de registro\n                     Swal.fire({\n                    title: \"✅\",\n                    text: \"Te has inscrito en el concurso con éxito\",\n                    confirmButtonText: \"O.K\",\n                    timer: 3000,\n                                  });\n                    \n                })\n                //Recogemos posibles errores\n                .catch(function(error){\n                    console.log(error);\n\n                    Swal.fire({\n                    title: \"⚠️\",\n                    text: \"Ha habido un error, es posible que ya estés inscrito\",\n                    confirmButtonText: \"O.K\",\n                    timer: 3000,\n                });\n                })\n                \n            \n        \n\n            }}};\n\n\n/* FUNCIÓN PARA TRAER FECHA Y UTILIZAR EN CLASES DINÁMICAS pendiente de funcionamiento \ngetTodayDate (datetoday) {\n  datetoday = new Date();\n  return format(datetoday, 'yyyy-MM-dd HH:mm:ss');\n},  */\n\n</script>\n\n<style>\n.green {\n  color: green;\n}\n\n.red {\n  color: red;\n}\n.concursoscontenedor {\n  box-shadow: 1px 1px 1px 1px #888888;\n  padding: 3em;\n  width: 300px;\n  margin: 10px auto;\n  border-radius: 20px;\n}\n\n.contenedor {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n  justify-content: space-around;\n  width: 90%;\n  margin: 10px auto;\n}\nimg {\n  width: 250px;\n  height: 250px;\n  border-radius: 20px;\n  transition: transform 0.5s ease-in-out;\n}\nimg:hover {\n  transform: scale(1.3);\n}\n\n#formulariobusqueda {\n  padding: 2em;\n}\n</style>"],"sourceRoot":"src/components"}]}