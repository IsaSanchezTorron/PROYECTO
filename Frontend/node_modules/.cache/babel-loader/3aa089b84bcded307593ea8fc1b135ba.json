JA,IAOA,QAAA,CAAA,MAAA,CAAA,WAAA,GAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAAA,WAAA,EAAA,CARA;AAAA,OADA,CAUA;AACA;AAXA,OAAA;AAaA;AArBA,GAhBA;AAwCA,EAAA,OAAA,EAAA;AAGA;AAEA,IAAA,kBALA,8BAKA,QALA,EAKA;AAEA,UAAA,IAAA,GAAA,IAAA,CAFA,CAIA;;AACA,UAAA,KAAA,GAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,GAAA,YAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,MAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA,CAAA,eAAA,qBAAA,KAAA;AAEA,MAAA,IAAA,CAAA,IAAA,CAAA;AACA,QAAA,KAAA,EAAA,IADA;AAEA,QAAA,IAAA,EAAA,uCAFA;AAGA,QAAA,gBAAA,EAAA,IAHA;AAIA,QAAA,kBAAA,EAAA,SAJA;AAKA,QAAA,iBAAA,EAAA,SALA;AAMA,QAAA,iBAAA,EAAA,yBANA;AAOA,QAAA,gBAAA,EAAA;AAPA,OAAA,EAQA,IARA,CAQA,UAAA,MAAA,EAAA;AACA,YAAA,MAAA,CAAA,KAAA,EAAA;AAEA,UAAA,KAAA,CACA,IADA,CAEA,mDACA,QAAA,CAAA,WAHA,EAIA;AACA,YAAA,WAAA,EAAA,IAAA,CAAA,WADA;AAEA,YAAA,UAAA,EAAA;AAFA,WAJA,EASA,IATA,CASA,UAAA,QAAA,EAAA;AAGA;AACA,YAAA,IAAA,CAAA,IAAA,CAAA;AACA,cAAA,KAAA,EAAA,GADA;AAGA,cAAA,IAAA,EAAA,0CAHA;AAIA,cAAA,iBAAA,EAAA,KAJA;AAKA,cAAA,KAAA,EAAA;AALA,aAAA;AAOA,WApBA,EAqBA;AArBA,WAsBA,KAtBA,CAsBA,UAAA,KAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AAEA,YAAA,IAAA,CAAA,IAAA,CAAA;AACA,cAAA,KAAA,EAAA,IADA;AAEA,cAAA,IAAA,EAAA,sDAFA;AAGA,cAAA,iBAAA,EAAA,KAHA;AAIA,cAAA,KAAA,EAAA;AAJA,aAAA;AAMA,WA/BA;AAgCA;;AAAA;AACA,OA5CA;AA6CA;AA3DA;AAxCA,CAAA","sourcesContent":["<template>\n  <div>\n    <!--Encabezado de p√°gina -->\n    <h2>Todos los concursos hasta la fecha</h2>\n    <!-- Forumulario de b√∫squeda -->\n    <div id=\"formulariobusqueda\">\n      <label for=\"bySearch\">B√∫squeda</label>\n      <br />\n      <input\n        v-model=\"search\"\n        id=\"search\"\n        name=\"bySearch\"\n        type=\"search\"\n        placeholder=\"üîç Nombre/Descripci√≥n...\"\n      />\n    </div>\n\n    <div class=\"contenedor\">\n      <br />\n      <!-- Contenedor para dar formato a la ficha de productos, lo cargamos con el array de productos filtrados -->\n      <div class=\"concursoscontenedor\" v-for=\"concurso in concursosFiltrados\" :key=\"concurso.id\">\n        <p>\n          üìå\n          {{ concurso.id_concurso }}\n        </p>\n        <h3>\n          <b>{{ concurso.nombre }}</b>\n        </h3>\n        <p>\n          <img :src=\"concurso.url_foto\" />\n        </p>\n\n        <!-- REVISAR estas clases din√°micas dependientes de FECHA -->\n        <!--  <p\n          :class=\"{\n            green: concurso.fecha_final >= 'datetoday',\n            red: concurso.fecha_final <= 'datetoday',\n          }\"\n        >-->\n\n        <p style=\"color:red\">\n          <b>üìÜ Cierre de suscripci√≥n:</b>\n          {{ concurso.fecha_final.slice(0, 10) }}\n        </p>\n\n        <p>\n          <b>üè† Modalidad:</b>\n          {{ concurso.modalidad }}\n        </p>\n\n        <p>\n          <b>üé≠ Genero:</b>\n          {{ concurso.genero }}\n        </p>\n\n        <p v-if=\"concurso.ciudad\">\n          <b>üåÜ Ciudad:</b>\n          {{ concurso.ciudad }}\n        </p>\n        <p v-if=\"concurso.fecha_asignacion_ganador\">\n          <b>üìÖ Publicaci√≥n de ganadores:</b>\n          {{ concurso.fecha_asignacion_ganador }}\n        </p>\n        <p v-if=\"concurso.nombre_ganador\">\n          <b>ü•á Ganador:</b>\n          {{concurso.nombre_ganador}}\n        </p>\n        <p v-if=\"concurso.valoracion\">\n          <b>üå† Valoraci√≥n media:</b>\n          {{ concurso.valoracion }}\n        </p>\n        <!-- Con una clase din√°mica manejo los colores en funci√≥n de la vigencia del concurso -->\n\n        <!-- El bot√≥n de inscribir DE MENTIRIJILLAS AUN, hace una llamada a la funci√≥n que nos env√≠a un Sweet Alert -->\n\n        <button @click=\"openModal()\">VER BASES</button>\n        <br />\n        <br />\n        <button @click=\"confirmInscription(concurso)\">QUIERO INSCRIBIRME</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// IMPORTAMOS PARA\n// enviar mensajes custom\nimport Swal from \"sweetalert2\";\n// manejo de rutas y endpoints\nimport axios from \"axios\";\n// componentes internos\n\nexport default {\n  name: \"listallconcourses\",\n  props: {\n    // Le indicamos que est√° recibiendo un array.\n    concursos: Array,\n  },\n\n  data() {\n    return {\n      // Inicializamos un string vac√≠o que contendr√° la b√∫squeda.\n      search: \"\",\n\n      id: null,\n    };\n  },\n\n  computed: {\n    //FUNCI√ìN PARA FILTRAR PRODUCTOS\n    concursosFiltrados() {\n      // Si en la b√∫squeda no hay nada nos devuelve todo.\n      if (!this.search) {\n        return this.concursos;\n        console.log(this.concursos);\n      }\n      return this.concursos.filter(\n        (concurso) =>\n          concurso.nombre.toLowerCase().includes(this.search.toLowerCase()) ||\n          concurso.descripcion\n            .toLowerCase()\n            .includes(this.search.toLowerCase()) ||\n          concurso.modalidad\n            .toLowerCase()\n            .includes(this.search.toLowerCase()) ||\n          concurso.genero.toLowerCase().includes(this.search.toLowerCase())\n        //Ojo aqu√≠, pendiente que funcione la b√∫squeda por ciudad.\n        // concurso.ciudad.toLowerCase().includes(this.search.toLowerCase())\n      );\n    },\n  },\n\n  methods: {\n\n\n    // M√âTODO PARA EL BOT√ìN DE INSCRIPCI√ìN EN CONCURSO.\n\n    confirmInscription(concurso) {\n\n      const self = this;\n      \n      // Cojo token e id.\n      const token = localStorage.getItem(\"token\");\n      const data = localStorage.getItem(\"id\");\n      axios.defaults.headers.common[\"Authorization\"] = `Bearer ${token}`;\n\n      Swal.fire({\n        title: \"ü§ì\",\n        text: \"¬øQuieres suscribirte a este concurso?\",\n        showCancelButton: true,\n         confirmButtonColor: \"#1CA1F2\",\n        cancelButtonColor: \"#EB5784\",\n        confirmButtonText: \"S√≠, quiero suscribirme.\",\n        cancelButtonText: \"Volver\",\n      }).then(result => {\n        if (result.value) {\n\n      axios\n        .post(\n          \"http://localhost:3003/concursos/inscripciones/\" +\n            concurso.id_concurso,\n          {\n            id_concurso: self.id_concurso,\n            id_usuario: data,\n          }\n        )\n        .then(function (response) {\n        \n          \n          // Enviamos mensaje de confirmaci√≥n de inscripci√≥n\n          Swal.fire({\n            title: \"‚ùì\",\n            \n            text: \"Te has inscrito en el concurso con √©xito\",\n            confirmButtonText: \"O.K\",\n            timer: 3000,\n          });\n        })\n        //Recogemos posibles errores\n        .catch(function (error) {\n          console.log(error.response.data.message);\n\n          Swal.fire({\n            title: \"‚ö†Ô∏è\",\n            text: \"Ha habido un error, es posible que ya est√©s inscrito\",\n            confirmButtonText: \"O.K\",\n            timer: 3000,\n          });\n        });\n    };\n      },\n      )},\n  },\n};\n</script>\n\n<style scoped>\n.green {\n  color: green;\n}\n\n.red {\n  color: red;\n}\n.concursoscontenedor {\n  box-shadow: 0 0 10px rgb(12, 12, 12);\n  padding: 3em;\n  width: 300px;\n  margin: 10px auto;\n  border-radius: 20px;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-around;\n}\n\n.contenedor {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n  justify-content: space-around;\n  width: 90%;\n  margin: 10px auto;\n}\nimg {\n  width: 250px;\n  height: 250px;\n  border-radius: 20px;\n  transition: transform 0.5s ease-in-out;\n}\nimg:hover {\n  transform: scale(1.3);\n}\n\n#formulariobusqueda {\n  padding: 2em;\n}\n\nh3 {\n  text-transform: uppercase;\n  font-size: 1.4em;\n}\n</style>\n"],"sourceRoot":"src/components"}]}